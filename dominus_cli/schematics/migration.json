{
    "type": "file",
    "name": "{{moduleName}}{{currentTimestamp}}",
    "extension": "php",
    "allowEmptyName": true,
    "overwriteIfExists": false,
    "template": "migration.txt",
    "destinationPath": "Migrations",
    "requiredTemplatePlaceholders": [
        "{{moduleName}}",
        "{{currentTimestamp}}"
    ]
}